{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": false,
    "lib": ["ES2021", "WebWorker"],
    "allowJs": true,
    "outDir": "dist",
    "isolatedModules": true,
    "noEmit": false,
    "noImplicitAny": false,
    "types": ["@cloudflare/workers-types"],
    "paths": {
      "undici-types": ["./node_modules/undici-types/index.d.ts"]
    }
  },
  "include": ["src/worker.js"],
  "exclude": ["node_modules"]
}
